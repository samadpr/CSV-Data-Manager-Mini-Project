@model List<Dictionary<string, string>>
@{
    ViewData["Title"] = "Manage Data";
    Layout = "~/Views/Shared/_PageLayout.cshtml";
}

<main id="main" class="main">
    <div class="pagetitle">
        <h1>Manage Data</h1>
        <nav>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a asp-controller="Page" asp-action="Index">Dashboard</a></li>
                <li class="breadcrumb-item active">ManageData</li>
            </ol>
        </nav>
    </div>
    @if (Model.Count == 0)
    {
        <p>No data found for the uploaded files.</p>
    }
    else
    {
        <table class="table table-bordered">
            <thead>
                <tr>
                    @foreach (var header in Model.FirstOrDefault()?.Keys)
                    {
                        <th>@header</th>
                    }
                </tr>
            </thead>
            <tbody>
                @foreach (var row in Model)
                {
                    <tr>
                        @foreach (var value in row.Values)
                        {
                            <td>@value</td>
                        }
                    </tr>
                }
            </tbody>
        </table>
    }
</main>
